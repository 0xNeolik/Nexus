<section id="third-section">
  <h1 class="text-center">Our Cybers are around the World!
  </h1>
  <div class="row justify-content-between">
    <input type="text" value='{{user.id}}' id="userID" hidden>
      {{#if isBusiness}}
        <div id="MapBusiness" class="col-7"></div>
        <div class="col-3">
          <h1>You own Cybers</h1>
            <ul>
                {{#each cyberFiltered}}
                  <li><a href="/cyber/details-cyber?id={{id}}">{{name}}</a></li>
                {{/each}}
            </ul>
        </div>  
      {{else}}
        <div id="mapIndex" class="col-7"></div>
        <div class="col-3">
          <h1>All our Cybers</h1>
          <ul>
            {{#each cyber}}
              <li><a href="/cyber/details-cyber?id={{id}}">{{name}}</a></li>
            {{/each}}
          </ul>
      {{/if}}
  </div>
</section>

